{
  "version": 2,
  "buildCommand": "npm install & npm run prisma:generate & npm run build",
  "builds": [
    {
      "src": "my-resumes-back/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "my-resumes-back/dist/main.js"
    }
  ]
}
